{ 
  "name": "Activemq chicken table",
  "type": "logs",
  "page": "analytics",
  "query": "table",
  "timerange": {
    "interval_ms": 345678
  },
  "visible_facets": ["status", "@http.referer_details.path", "@duration"],
  "options": {
    "group_bys": [
      { "facet": "status" },
      { "facet": "@http.referer_details.path" }
    ],
    "aggregations": [
      { "metric": "@http.referer_details.path", "type": "cardinality" },
      { "metric": "status", "type": "cardinality" }
    ],
    "limit": "5",
    "order": "top",
    "widget": "query_table"
  }
}
